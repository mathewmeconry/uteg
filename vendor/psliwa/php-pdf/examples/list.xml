<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pdf SYSTEM "%resources%/dtd/doctype.dtd">
<pdf>
    <dynamic-page>
    	<div>
	    	<p>Position: outside</p>
	    	<ul list-position="outside">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	<p>Position: inside</p>
	    	<ul>
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	<p>Type: square (only using font with utf-8 support)</p>
	    	<ul type="square">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	<p>Type: disc (only using font with utf-8 support)</p>
	    	<ul type="disc">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	<p>Type: none</p>
	    	<ul type="none">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	<p>Custom char as type ("+")</p>
	    	<ul type="+">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	
	    	<p>Type: image</p>
	    	<ul type="image" image="dir:list_icon.jpg">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>

	    	<p>Ordered list</p>
	    	<ol>
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ol>
	    	
	    	<p>Ordered list</p>
	    	<ol type="decimal-leading-zero" list-position="outside">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ol>
	    	
	    	<p>Ordered list</p>
	    	<ul type="lower-alpha">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
	    	
	    	<p>Ordered list</p>
	    	<ul type="upper-alpha">
	    		<li>Some text</li>
	    		<li>some another text</li>
	    	</ul>
    	</div>
    </dynamic-page>
</pdf>