{% trans_default_domain 'UtegBase' %}}
{% form_theme form 'bootstrap_3_layout.html.twig' %}

<form class="inviteNewForm" method="post" action="{{ path(target) }}" class="form-horizontal">
    {{ form_row(form.club) }}
    {{ form_row(form.club) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.email) }}
    {{ form_row(form.street) }}
    {{ form_row(form.city) }}
    {{ form_row(form.zipcode) }}
    {{ form_row(form._token) }}
</form>