{% trans_default_domain 'uteg' %}
{% form_theme form 'form/formTheme_horizontal.html.twig' %}

<form id="departmentEditForm" method="post" action="{{ path(target, { 'compid': app.request.get('compid')}) }}"
      class="form-horizontal">

    {{ form_row(form.date) }}

    {{ form_row(form.competitionPlace) }}

    {{ form_row(form.gender) }}

